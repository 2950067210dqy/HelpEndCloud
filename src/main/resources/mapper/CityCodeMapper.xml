<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dqy.helpeachothers.mapper.CityCodeMapper" >
    <select id="findAll" resultType="com.dqy.helpeachothers.entity.CityCode" >
        select * from citycode
    </select>

    <select id="getByAdcode" resultType="com.dqy.helpeachothers.entity.CityCode" >
        select * from citycode where  adcode = ${adcode}
    </select>
</mapper>
